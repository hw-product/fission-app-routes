<%=
  create_window(
    :name => 'configurator_edit',
    :title => "Edit rule: #{@name}",
    :size => 'normal',
    :content => render(
      :partial => 'route_config_basic',
      :locals => {
        :name => @name,
        :description => @description,
        :configs => @configs,
        :set_configs => @set_configs,
        :rules => @rules
      }
    )
  )
%>
enable_editables();
